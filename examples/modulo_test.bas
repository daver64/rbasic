// Test modulo operator specifically
print("Testing modulo operator");

var counter = 0;

while (counter < 1) {
    print("Loop iteration " + str(counter));
    
    var modulo_test = 120 % 400;
    print("modulo_test = " + str(modulo_test));
    
    counter = counter + 1;
}

print("Modulo test complete");